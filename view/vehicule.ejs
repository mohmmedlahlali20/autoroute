<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vehicles</title>
    <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
    <h1>Vehicles List</h1>
    <table>
        <thead>
            <tr>
                <th>ID</th>
                <th>Make</th>
                <th>Model</th>
                <th>Year</th>
            </tr>
        </thead>
        <tbody>
            <% vehicles.forEach(vehicle => { %>
                <tr>
                    <td><%= vehicle.id %></td>
                    <td><%= vehicle.louee %></td>
                    <td><%= vehicle.type %></td>
                    <td><%= vehicle.immatriculation %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
